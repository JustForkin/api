{
  "name": "jsbin-api",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "description": "jsbin's API (refactored)",
  "main": "lib/server.js",
  "scripts": {
    "pretest": "rm -rf jsbin-test.sqlite",
    "test":
      "NODE_ENV=test jest --setupTestFrameworkScriptFile=@remy/envy tests/**/*.test.js",
    "tunnel": "lt --port $(($(envy PORT)+1)) -s jsbin",
    "port-forward":
      "ssh -g -L $(($(envy PORT)+1)):localhost:$(envy PORT) localhost -N",
    "dev": "npm run dev-server & npm run port-forward & npm run tunnel",
    "dev-server":
      "GIT_REV=$(git rev-parse HEAD) NODE_ENV=development nodemon -w lib .",
    "start": "node ."
  },
  "keywords": ["jsbin"],
  "author": "Remy Sharp <remy@leftlogic.com>",
  "license": "MIT",
  "dependencies": {
    "@remy/envy": "^3.1.1",
    "body-parser": "^1.17.1",
    "express": "^4.15.4",
    "http-status-codes": "^1.2.0",
    "jsbin-id": "^1.0.0",
    "jsonwebtoken": "^7.4.2",
    "lodash.pick": "^4.4.0",
    "lru-cache": "^4.1.1",
    "morgan": "^1.8.1",
    "mysql2": "^1.5.1",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "request": "^2.81.0",
    "sequelize": "^4.5.0",
    "serve-favicon": "^2.4.5",
    "server-timings": "^1.0.4",
    "stripe": "^4.24.0",
    "undefsafe": "^2.0.1",
    "universal-analytics": "^0.4.15"
  },
  "devDependencies": {
    "@remy/eslint": "^1.3.0",
    "ajv": "^5.5.2",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.12.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "localtunnel": "^1.8.3",
    "sqlite3": "^3.1.13",
    "supertest": "^3.0.0"
  }
}
